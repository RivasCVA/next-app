{
  "extends": [
    "next/core-web-vitals",
    "airbnb-base",
    "airbnb-typescript/base",
    "prettier"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-restricted-exports": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
